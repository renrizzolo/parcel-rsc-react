{
  "name": "@renr/parcel-reporter-rsc-router",
  "version": "0.0.1",
  "author": "Ren Rizzolo",
  "license": "MIT",
  "description": "Parcel reporter that generates filesystem route definitions on build",
  "keywords": [
    "parcel",
    "rsc"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/renrizzolo/parcel-rsc-react.git"
  },
  "type": "module",
  "main": "src/index.ts",
  "files": [
    "src"
  ],
  "sideEffects": false,
  "exports": {
    "import": {
      "types": "./dist/esm/index.d.ts",
      "default": "./dist/esm/index.js"
    },
    "require": {
      "types": "./dist/esm/index.d.ts",
      "default": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "tsdown",
    "type-check": "tsgo --noEmit",
    "example:generate": "vite-node example/generate.ts"
  },
  "dependencies": {
    "@parcel/plugin": "^2.16.0",
    "tinyglobby": "^0.2.15",
    "@renr/parcel-rsc-router": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.15.30",
    "vite-node": "^3.2.4"
  }
}
