{
  "name": "@renr/parcel-reporter-rsc-router",
  "version": "1.0.0-alpha.6",
  "author": "Ren Rizzolo",
  "license": "MIT",
  "description": "Parcel reporter that generates filesystem route definitions on build",
  "keywords": [
    "parcel",
    "rsc"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/renrizzolo/parcel-rsc-react.git"
  },
  "type": "module",
  "main": "src/index.ts",
  "files": [
    "src"
  ],
  "sideEffects": false,
  "exports": {
    "default": {
      "types": "./dist/esm/index.d.ts",
      "default": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "build": "tsdown",
    "type-check": "tsgo --noEmit"
  },
  "dependencies": {
    "@parcel/diagnostic": "^2.16.1",
    "@parcel/plugin": "^2.16.1",
    "dedent": "^1.7.0",
    "tinyglobby": "^0.2.15"
  },
  "peerDependencies": {
    "@renr/parcel-rsc-router": "1.0.0-alpha.3"
  },
  "devDependencies": {
    "@types/node": "^22.19.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
